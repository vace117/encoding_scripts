#!/bin/sh

# Any2Dvd-0.30
# A script to automate the creation of PAL/NTSC DVDs from any PC or streaming multimedia file(s), complete with DVD menus
# Supports URL streams & filetypes with multiple video, audio & subtitle tracks
# Optionally compresses the encoded file(s) by reducing the resolution & video bitrate (--compress=yes)
# Optionally creates a 5.1 'surround' sound audio track if one does not already exist in the original input file(s) (enabled by default)
# Author: Rick Harris  rickfharris@yahoo.com.au
#
# DVD-menu creation is heavily based around Wolfgang Wershofen's work with 'DVDWizard', located here ->
# 	http://www.wershofen.de/downloads/dvdwizard.tar.gz
# DVDWizard is also part of a suite of tools called 'udrecsuite' located here ->
#	http://udrecsuite.sourceforge.net/
#
# Pre-requisite:	Fast computer with LOTS of diskspace
#
# Some required packages:
#	* GNU Ghostscript Fonts Package			-> ftp://mirror.cs.wisc.edu/pub/mirrors/ghost/fonts/ghostscript-fonts-std-8.11.tar.gz
#		Re-compile Imagemagick with Ghostscript support once installed
#		In a terminal type `convert -list type`, if the fonts package is installed correctly, it will show a list of fonts
#	* Imagemagick built with Ghostscript support	-> http://www.imagemagick.org/
#
################################################################################################

VAR_DATAFILE="/tmp/srcvar.dat"
rm $VAR_DATAFILE
touch $VAR_DATAFILE

## Any2Vob default variables ##
FORMAT="pal"
PREVIEW="no"
AUD_FORMAT="AC3"
AUD_FMT="ac3"
FPS="25.000"
WIDTH="720"
HEIGHT="576"
MENCODER="no"
SURROUND="yes"
HAS_SURROUND="no"
COMPRESS="no"
ASPECT="3"
ASPECT_E="16/9"
ASPECT_T="16:9"
HAS_DTS="no"
KEEP_DTS="yes"
SKIP_COMM="no"
MPEGTS_PIDS="no"
LOG="no"
LOGFILE="any2vob.log"
SUBFONT="Vera.ttf"
SUBFONTSIZE="18.0"
SUBCHARACTERSET="ISO8859-1"

## Any2Dvd default variables ##
IM_VERSION=`convert -version 2> /dev/null | grep ^Version: | cut -d' ' -f 3 | cut -d. -f 1`
workDir="tmp_any2dvd"
LOGFILE="$workDir"/any2dvd.log
MENU="yes"
ISO="no"
BURN="no"
PALETTE="$workDir"/palette.rgb
TVSIZE="720x576"
DVDAUDIO="en"
TITLE_FONTSIZE=28			# Title fontsize
TITLE_FONTPATH="Helvetica"		# Title font
BUTTON_FONTSIZE=28			# Menu fontsize
BUTTON_FONTPATH="Helvetica-Bold"	# Menu font
MENU_FONTSIZE=22			# VTSM fontsize
MENU_FONTPATH="Helvetica"		# VTSM font
CHAPTERTIME=5				# In minutes
VMGM_MAXTITLE=6
VMGMBG=""
VTSMBG=""
CHAPTERSPERROW=2
MJPEG_VERSION=`mplex 2>&1 | grep version | awk '{print $4}'`
if [ "$MJPEG_VERSION" \< "1.8.0" ]; then 
	PPMTOY4M_OPTS="-S 420_mpeg2"
else
	PPMTOY4M_OPTS="-S 420mpeg2"
fi
DE=Deutsch
EN=English

trap "echo Interrupted; reset; exit 2" 2

warn () {
echo ""
echo -e "\e[33;01m $1\e[m\c"	# Amber
shift; echo " $*"
echo ""
return 0
}
good () {
echo ""
echo -e "\e[32;01m $1\e[m\c"	# Green
shift; echo " $*"
echo ""
return 0
}
errmsg () {
echo ""
echo -e "\e[31;01m $1\e[m\c"	# Red
shift; echo " $*"
echo ""
}

help_text () {
cat <<EOF
Any2Dvd-0.30
Usage: $0 [options] -i [input_file(s)]

Encoding options:
--format=[pal|ntsc]			Format to encode to					(Default: PAL)
--preview=[yes|no]			Preview video while encoding (slower)			(Default: No)
--audio_format=[ac3|mp2|pcm16|pcm24]	Audio format to encode to				(Default: AC3)
--surround=[yes|no]			Encode audio track to 5.1 surround sound		(Default: Yes)
--compress=[yes|no]			Halve the encoding time & encoded filesize		(Default: No)
--aspect=[4:3|16:9]			Aspect ratio to encode to				(Default: 16:9)
--keep_dts=[yes|no]			Preserve DTS audio track if detected			(Default: Yes)
--log=[yes|no]				Turn logging on (any2vob.log)				(Default: No)

Subtitle options:
-s [subtitle file(s)]			Encode external subtitle file(s)
--subfont=[fontname.ttf]		Font to use in subtitles				(Default: Vera.ttf)
--subfontsize=[fontsize]		Set fontsize in subtitles				(Default: 18.0)
--subcharacterset=[ISO-codepage]	Set subtitle language codepage for your country		(Default: ISO8859-1)

DVD Options:
--menu=[yes|no]				Choose whether or not to build DVD Menus		(Default: Yes)
--iso=[filename.iso]			Create DVD ISO image for later use			(Default: None)
--burn=[yes|no]				Choose whether or not to burn DVD			(Default: No)
--palette=[file.rgb]			Name of palette file (for subtitle colours)		(Default: Internal)

Main DVD-Menu (VMGM) Options:
--animate=[yes|no]			Create an animated VMGM Main Menu (slow but pretty)	(Default: No)
--vmgm_bg=[file.png|jpg|bmp|ppm]	Background-Picture for VMGM Main Menu			(Default: Internal)
--vmgm_audio=[file.mp3|ogg|wav]		Audio for VMGM Main Menu				(Default: None)

Chapter-Menu (VTSM) Options:
--chapter_points=[<X> minutes]		Chapterpoints time period in minutes			(Default: Every 5 mins)
--vtsm_bg=[file.png|jpg|bmp|ppm]	Background-Picture for VTSM Title Menu			(Default: Internal)
--vtsm_audio=[file.mp3|ogg|wav]		Audio for VTSM Title Menu				(Default: None)

Miscellaneous options:
--workdir=[/path/to/dir]		Directory to use for temp files & final VOBS/DVD	(Default: ./tmp_any2dvd/)
--skip_comm=[yes|no]			Skip commercials for MythTV recorded video		(Default: No)
--mpegts_allpids=[yes|no]		Encode all PIDs from MPEG-TS capture files		(Default: No (First PID only))

NOTE:	* When specifying external subtitle file(s) with the '-s' option in batch input_file conversion mode,
		the subtitles will be applied to the first input_file only
	* Encoding to DTS audio is not supported, however if a DTS audio track is found it will be preserved,
		unless otherwise specified with '--keep_dts=no', in which case it will be converted to either
		AC3 or the format specified with '--audio_format=...'
	* MP2 & PCM audio format encodings are strictly stereo only
EOF
exit 0
}

if [ -z "$1" ]; then
help_text
else
until [ -z "$1" ] ; do
 case "$1" in
--format=ntsc) shift; FORMAT="ntsc"; FPS="29.97"; TVSIZE="720x480"; HEIGHT="480" ;;
--audio_format=mp2) shift; AUD_FORMAT="MP2"; AUD_FMT="mp2" ;;
--audio_format=pcm16) shift; AUD_FORMAT="PCM 16-bit"; AUD_FMT="pcm16"; PCM_ARGS="-L 48000:2:16" ;;
--audio_format=pcm24) shift; AUD_FORMAT="PCM 24-bit"; AUD_FMT="pcm24"; PCM_ARGS="-L 48000:2:24" ;;
--surround=no) shift; SURROUND="no" ;;
--compress=yes) shift; COMPRESS="yes";;
--aspect=4:3) shift; ASPECT="2"; ASPECT_E="4/3"; ASPECT_T="4:3" ;;
--keep_dts=no) shift; KEEP_DTS="no" ;;
--preview=yes) shift; PREVIEW="yes" ;;
--log=yes) shift; LOG="yes" ;;
--subfont=*) SUBFONT=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--subfontsize=*) SUBFONTSIZE=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--subcharacterset=*) SUBCHARACTERSET=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--workdir=*) workDir=`echo "$1" | sed 's/--workdir=/ /' | sed 's/^[ \t]*//'`
		LOGFILE="$workDir"/any2dvd.log	# Update path to logfile with new $workDir
	if [ ! -d "$workDir" ]; then
		errmsg Error: Directory "$workDir" does not exist!
		exit
	fi ;
	shift ;;
--menu=no) shift; MENU="no" ;;
--iso=*) ISO=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--burn=yes) shift; BURN="yes" ;;
--palette=*) PALETTE=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--animate=yes) shift; ANIMATE="yes" ;;
--vmgm_bg=*) VMGMBG=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--vmgm_audio=*) VMGMAUD=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--vtsm_bg=*) VTSMBG=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--vtsm_audio=*) VTSMAUD=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--chapter_points=*) CHAPTERTIME=`echo $1 | sed 's/=/ /' | awk '{print $2}'`; shift ;;
--skip_comm=yes) shift; SKIP_COMM="yes" ;;
--mpegts_allpids=yes) shift; MPEGTS_PIDS="yes" ;;
-s) shift;	EXTSUB="1"
		SUBS="$@"
		SUBS=`echo $SUBS | sed 's/\(\([^ -i ]* -i \)\).*/\1/' | sed 's/.\{3\}$//'`
		while [ "$1" != "-i" ]; do
			if [ ! -z "$1" ]; then
				let k=$k+1
				eval "subs_file_$k=\"$1\""
				subs_file_list="$subs_file_list $k"
				shift
			fi
                done ;;
-i) shift; input_files="$@"
		while [ "$#" -gt 0 ]; do
			if [ ! -z "$1" ]; then
				let j=$j+1
				eval input_file_$j=\"$1\"
				input_file_list="$input_file_list $j"
				shift
			fi
		done ;;
-h|-help|--help) help_text ;;
*) shift ;;
 esac
done
fi

if [ "$workDir" = "tmp_any2dvd" ] && [ ! -d "tmp_any2dvd" ]; then mkdir tmp_any2dvd; fi
DESTDIR="$workDir/DVD"
XMLFILE="$workDir/xml/any2dvd.xml"

TOOLKIT=([1]=convert [2]=any2vob [3]=ffmpeg [4]=mplayer [5]=mplex [6]=uudecode [7]=growisofs)
TOOLURL=([1]=http://www.imagemagick.org/ [2]=http://any2dvd.sourceforge.net/ [3]=http://ffmpeg.sourceforge.net/ [4]=http://www.mplayerhq.hu/ [5]=http://mjpeg.sourceforge.net/ [6]=http://www.gnu.org/software/sharutils/ [7]=http://fy.chalmers.se/~appro/linux/DVD+RW/)

testprog () {
for p in $(echo $PATH | sed "s/:/\ /g"); do
if [ -x "$p/$1" ]; then
return 0
fi
done
errmsg Error: "Program '"$1"' is missing, please install it first ->  "${TOOLURL["$ARRAY"]}""
echo -e "	${TOOLMSG["$ARRAY"]}"
return 1
}

check_tools () {
ARRAY_NUM="${#TOOLKIT[@]}"
ARRAY="1"
while [ "$ARRAY" -le "$ARRAY_NUM" ]; do
testprog ${TOOLKIT["$ARRAY"]} || exit 0;
let ARRAY=$ARRAY+1
done
}

fontpath () {
GS_XML=`convert -list type | grep Path: | awk '{print $2}'`
if [ ! -f "$GS_XML" ] || [ ! -f "`cat $GS_XML | grep glyphs | tail -n1 | cut -d= -f2 | sed 's/\"//g'`" ]; then
	errmsg Error: "GNU-Ghostscript fonts were not found"
	echo " Please install GNU-Ghostscript fonts package found here ftp://mirror.cs.wisc.edu/pub/mirrors/ghost/fonts/"
	echo "  Then re-install Imagemagick with Ghostscript support"
	echo
	exit
#	TFONT="VeraBd.ttf"	# Title font
#	MFONT="VeraBd.ttf"	# Menu font
#	for i in `cat /etc/X11/fs/config | grep -E "TTF|TrueType|truetype" | grep / | awk -F"," '{for(i=1;i<=NF;i++) print $i}' | \
#		cut -d= -f2 | awk '{print $1}' | grep -E "TTF|TrueType|truetype"`; do
#			j=$i/"$TFONT"
#			k=$i/"$MFONT"
#			if [ -f "$j" ]; then
#				TITLE_FONTPATH=$j;
#				BUTTON_FONTPATH=$k;
#			fi
#	done
fi
}

spinner () {
spinners="| / - .\\"
PID=`ps aux | grep "$PROC" | grep -v grep | awk '{print $2}'`
until [ -z "$PID" ]; do
	for spin in $spinners ; do
		echo -n $spin
		sleep 0.5
		echo -e -n "\b"
	done
	PID=`ps aux | grep "$PROC" | grep -v grep | awk '{print $2}'`
done
}

##############################################################################################

## Call map of functions ##
# main
# |_mk_vob						# Encode multimedia files to DVD audio/video using any2vob
# |_mk_vmgm _OR_ mk_vmgm_animate			# Build VMGM Menu
# |	|_input_info					# Extract info from VOB files, resolution/fps/aspect etc.
# |	|_audio_probe					# Extract audio info from VOB files
# |	|_dvdcpics					# Extract Chapter points snapshots
# |	|_menu_audio					# Create audio for VMGM/VTSM Menus
# |	|_menu_bg					# Create VMGM Background
# |	|_mk_picts					# Create Button Pictures for VMGM-Options
# |	|_mk_vts_tile _OR_ mk_vts_tile_animate		# Create VMGM with VTS-Tile thumbnails, buttons and titles
# |_mk_vtsm						# Build VTSM Menu
# |	|_menu_bg					# Create VTSM Background
# |	|_mk_vtsm_audio					# Create VTSM Audio Selection Menu
# |	|	|_menu_bg				# Create VTSM Audio Selection Background
# |	|_mk_vtsm_subs					# Create VTSM Subtitle Selection Menu
# |	|	|_menu_bg				# Create VTSM Subtitle Selection Background
# |_mk_dvdauthor					# Create DVD file structure ready for image creation & burn

mk_vob () {						# Encode multimedia files to DVD audio/video
for i in $input_file_list; do
		var="\$input_file_$i";
		eval "file=\"$var\"";
		a2v_file_list="$a2v_file_list \"$file\""
done
if [ ! -z "$SUBS" ]; then
SUB_ARGS="--subfont=$SUBFONT --subfontsize=$SUBFONTSIZE --subcharacterset=$SUBCHARACTERSET -s $SUBS"
fi
good Running: "any2vob --workdir=$workDir --format=$FORMAT --audio_format=$AUD_FMT --surround=$SURROUND --compress=$COMPRESS --aspect=$ASPECT_T --keep_dts=$KEEP_DTS --preview=$PREVIEW --log=$LOG  --skip_comm=$SKIP_COMM --mpegts_allpids=$MPEGTS_PIDS $SUB_ARGS -i $input_files"
CMD="any2vob --workdir=\"$workDir\" --format=$FORMAT --audio_format=$AUD_FMT --surround=$SURROUND --compress=$COMPRESS --aspect=$ASPECT_T --keep_dts=$KEEP_DTS --preview=$PREVIEW --log=$LOG --skip_comm=$SKIP_COMM --mpegts_allpids=$MPEGTS_PIDS $SUB_ARGS -i $a2v_file_list"
eval $CMD
}

input_info () {						# Extract info from VOB files, resolution/fps/aspect etc.
SRC_WIDTH=`tcprobe -i "$1" 2> /dev/null | grep import | awk '{print $5}' | cut -dx -f1`
SRC_HEIGHT=`tcprobe -i "$1" 2> /dev/null | grep import | awk '{print $5}' | cut -dx -f2`
SRC_FPS=`tcprobe -i "$1" 2> /dev/null | grep "\-f" | awk '{print $4}'`
ASPECTX=`tcprobe -i "$1" 2> /dev/null | grep aspect | awk '{print $3}' | cut -d: -f1`
ASPECTY=`tcprobe -i "$1" 2> /dev/null | grep aspect | awk '{print $3}' | cut -d: -f2`
SUB_NUM=`tcprobe -H300 -i "$1" 2>&1 | grep subtitle | cut -d\( -f2 | cut -d\) -f1`
# This gets ugly, but mplayer's ID_LENGTH in seconds is totally broken for MPEG2 #
TOTAL_DURATION=`ffmpeg -i "$1" 2>&1 | grep Duration | cut -d, -f1 | awk '{print $2}'`
DURATION_HOURS=`echo "$TOTAL_DURATION" | cut -d, -f1 | cut -d: -f1 | awk '{print $1}' | sed -e 's/^0//'`
DURATION_MINS=`echo "$TOTAL_DURATION" | cut -d, -f1 | cut -d: -f2 | awk '{print $1}' | sed -e 's/^0//'`
DURATION_SECS=`echo "$TOTAL_DURATION" | cut -d, -f1 | cut -d: -f3 | awk '{print $1}' | sed -e 's/^0//'`
DURATION_MINS=`perl -le "print int( ( $DURATION_HOURS * 60 ) + ( $DURATION_SECS / 60 ) + $DURATION_MINS )"`
DURATION_SECS=`perl -le "print int( $DURATION_MINS * 60 )"`
}

audio_probe () {					# Extract audio info from VOB files
mplayer -vo dummy -identify "$1" 2>&1 | grep ID_AUDIO_ID | cut -d= -f2 > "$workDir"/src_audio.dat
astring=""	# Reset
acount=""
for j in `cat "$workDir"/src_audio.dat`; do
# Mplayer used to detect all here as both tcprobe & ffmpeg almost always mis-detect DTS as AC3 #
if [ ! -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "audio codec" | awk -F\( '{print $NF}' | grep "AC3-liba52"`" ] && \
	[ ! -z "`mplayer -vo dummy -aid $j -identify $1 2>&1 | grep "3f+2r+lfe"`" ]; then
        astring="$astring ac3+Dolby 5.1 Surround,"
elif [ ! -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "audio codec" | awk -F\( '{print $NF}' | grep "AC3-liba52"`" ] && \
	[ -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "3f+2r+lfe"`" ]; then
	astring="$astring ac3+Stereo,"
elif [ ! -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "audio codec" | awk -F\( '{print $NF}' | grep "MPEG layer-2"`" ]; then
        astring="$astring mp2+Stereo,"
elif [ ! -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "audio codec" | awk -F\( '{print $NF}' | grep "VOB LPCM"`" ]; then
        astring="$astring pcm+Stereo,"
elif [ ! -z "`mplayer -vo dummy -aid $j -identify "$1" 2>&1 | grep "audio codec" | awk -F\( '{print $NF}' | grep "DTS"`" ]; then
        astring="$astring dts+5.1 Surround,"
else
        astring="$astring ??+??,"
fi
done
astring_tmp=`echo "$astring" | sed 's/,*$//'`
astring="$astring_tmp"
acount=`echo $astring | awk -F"," '{print NF}'`
}

dvdcpics () {						# Extract Chapter points snapshots
CHAPTERPOINT=""		# Reset
SRC_ASPECT=`perl -le "print ( $ASPECTX / $ASPECTY )"`
SC_WIDTH=`perl -le "print int( $SRC_ASPECT * $HEIGHT )"`
firstframe=5	# Seconds
frames=60
CHAPTERTIME_SECS=`perl -le "print ( $CHAPTERTIME * 60 )"`
chapternum=1
mkdir -p "$workDir"/VTS-$VTS/tmp_cpics/
mkdir -p "$workDir"/VTS-$VTS/cpics/
echo
echo "Creating Titleset $VTS Chapter $chapternum"
mplayer -vf scale="$SC_WIDTH":"$HEIGHT" -ao null -ss $firstframe -frames $frames -vo jpeg:outdir=$workDir/VTS-$VTS/tmp_cpics/ "$1" &> /dev/null
	prefpic=`ls -S "$workDir"/VTS-$VTS/tmp_cpics/ | head -n1`
	mkdir -p $workDir/VTS-$VTS/cpics/
	mv $workDir/VTS-$VTS/tmp_cpics/$prefpic $workDir/VTS-$VTS/cpics/chapter-$chapternum.jpg
	rm -rf $workDir/VTS-$VTS/tmp_cpics/
	let chapternum=2
	let CHAPTERPOINT=$CHAPTERPOINT+$CHAPTERTIME_SECS
if [ "`mplayer -vo dummy -identify "$1" 2>&1 | grep ID_LENGTH | cut -d= -f2`" -gt 0 ]; then	# Check if muxed audio track is longer than video stream, otherwise $prefpic will return zero causing hang
	until [ $CHAPTERPOINT -ge $DURATION_SECS ]; do
		mkdir -p $workDir/VTS-$VTS/tmp_cpics/
		echo "Creating Titleset $VTS Chapter $chapternum"
		mplayer -vf scale="$SC_WIDTH":"$HEIGHT" -ao null -ss $CHAPTERPOINT -frames $frames -vo jpeg:outdir=$workDir/VTS-$VTS/tmp_cpics/ "$1" &> /dev/null
		prefpic=`ls -S "$workDir"/VTS-$VTS/tmp_cpics/ | head -n1`
		mkdir -p $workDir/VTS-$VTS/cpics/
		mv $workDir/VTS-$VTS/tmp_cpics/$prefpic $workDir/VTS-$VTS/cpics/chapter-$chapternum.jpg
		let chapternum=$chapternum+1
		let CHAPTERPOINT=$CHAPTERPOINT+$CHAPTERTIME_SECS
	rm -rf $workDir/VTS-$VTS/tmp_cpics/
	done
fi
}

menu_audio() {						# Create audio for VMGM/VTSM Menus
if [ ! -z $VMGMAUD ]; then
ffmpeg -y -i "$VMGMAUD" -ar 48000 -ab 384 -ac 2 "$workDir"/audio/vmgm-audio.ac3 &> /dev/null &
	sleep 5s
	IFS=" "
	PROC="ffmpeg"
	echo
	echo -e -n "\e[33;01m Creating VMGM Main Menu audio \e[m..."
	spinner
	echo -n "Done!"
	echo
	IFS="
"
else
ffmpeg -y -ar 48000 -ab 224 -ac 2 -t 5 "$workDir"/audio/vmgm-audio.ac3 &> /dev/null &
	sleep 5s
	IFS=" "
	PROC="ffmpeg"
	echo
	echo -e -n "\e[33;01m Creating empty Audio-Track for VMGM Main Menu \e[m..."
	spinner
	echo -n "Done!"
	echo
	IFS="
"
fi
if [ ! -z $VTSMAUD ]; then
ffmpeg -y -i "$VTSMAUD" -ar 48000 -ab 384 -ac 2 "$workDir"/audio/vtsm-audio.ac3 &> /dev/null &
	sleep 5s
	IFS=" "
	PROC="ffmpeg"
	echo
	echo -e -n "\e[33;01m Creating VTSM Chapter Menu audio \e[m..."
	spinner
	echo -n "Done!"
	echo
	IFS="
"
else
ffmpeg -y -ar 48000 -ab 224 -ac 2 -t 5 "$workDir"/audio/vtsm-audio.ac3 &> /dev/null &
	sleep 5s
	IFS=" "
	PROC="ffmpeg"
	echo
	echo -e -n "\e[33;01m Creating empty Audio-Track for VTSM Chapter Menu \e[m..."
	spinner
	echo -n "Done!"
	echo
	IFS="
"
fi
vmgm_audiotrack=\""$workDir/audio/vmgm-audio.ac3"\"
vtsm_audiotrack=\""$workDir/audio/vtsm-audio.ac3"\"
VMGMAUD_TOTAL_DURATION=`eval ffmpeg -i "$vmgm_audiotrack" 2>&1 | grep Duration | cut -d, -f1 | awk '{print $2}'`
VMGMAUD_DURATION_HOURS=`echo "$VMGMAUD_TOTAL_DURATION" | cut -d, -f1 | cut -d: -f1 | awk '{print $1}' | sed -e 's/^0//'`
VMGMAUD_DURATION_MINS=`echo "$VMGMAUD_TOTAL_DURATION" | cut -d, -f1 | cut -d: -f2 | awk '{print $1}' | sed -e 's/^0//'`
VMGMAUD_DURATION_SECS=`echo "$VMGMAUD_TOTAL_DURATION" | cut -d, -f1 | cut -d: -f3 | awk '{print $1}' | sed -e 's/^0//'`
VMGMAUD_DURATION_MINS=`perl -le "print int( ( $VMGMAUD_DURATION_HOURS * 60 ) + ( $VMGMAUD_DURATION_SECS / 60 ) + $VMGMAUD_DURATION_MINS )"`
VMGMAUD_DURATION_SECS=`perl -le "print int( $VMGMAUD_DURATION_MINS * 60 )"`
echo ""
}

menu_bg () {						# Prepare the background pictures for the VMGM & VTSM Menus
bgPic="$1"
# Set dimensions to PAL or NTSC
if [ "$FORMAT" == "ntsc" ]; then
normX=720
normY=480
densityX=81
densityY=72
else
normX=720
normY=576
densityX=75
densityY=80
fi
normSize="$normX"x"$normY"
normDensity="$densityX"x"$densityY"
tvX=`echo $TVSIZE | cut -dx -f 1`
tvY=`echo $TVSIZE | cut -dx -f 2`
let offsetX=($normX-$tvX)/2+1
let offsetY=($normY-$tvY)/2+1

# Create the default background
currDir=`pwd`
cd "$workDir"/VMGM
  sed 's/^X//' << 'SHAR_EOF' | uudecode &&
